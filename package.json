{
  "name": "cocos-skills",
  "version": "1.0.0",
  "description": "Cocos Creator HTTP API CLI Tool - TypeScript CLI for Cocos Creator editor automation",
  "type": "module",
  "bin": {
    "cocos-skills": "./scripts/dist/index.js"
  },
  "exports": {
    ".": "./scripts/dist/lib/client.js"
  },
  "scripts": {
    "build": "tsc -p cli/tsconfig.json && npm run copy-scripts && npm link",
    "dev": "tsc -p cli/tsconfig.json --watch",
    "prepublishOnly": "npm run build",
    "copy-scripts": "node copy-scripts.mjs",
    "clean": "node clean.mjs",
    "test": "vitest",
    "test:unit": "vitest run cli/tests/unit",
    "test:integration": "vitest run cli/tests/integration",
    "test:e2e": "vitest run cli/tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "files": [
    "scripts/dist",
    "skills"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.0.0",
    "msw": "^2.0.0",
    "typescript": "^5.7.3",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "cocos-creator",
    "cli",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.3",
    "string-random": "^1.0.0",
    "uuid": "^13.0.0"
  }
}
