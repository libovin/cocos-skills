{
  "name": "cocos-skills",
  "version": "1.0.0",
  "description": "Cocos Creator HTTP API CLI Tool - TypeScript port of Python scripts",
  "type": "module",
  "bin": {
    "cocos-skills": "./cli/dist/index.js"
  },
  "exports": {
    ".": "./cli/dist/lib/client.js"
  },
  "scripts": {
    "build": "tsc -p cli/tsconfig.json",
    "dev": "tsc -p cli/tsconfig.json --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "cli/dist",
    "scripts",
    "skills"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "cocos-creator",
    "cli",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.3",
    "string-random": "^1.0.0",
    "uuid": "^13.0.0"
  }
}
